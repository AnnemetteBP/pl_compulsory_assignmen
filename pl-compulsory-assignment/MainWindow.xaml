<Window x:Class="pl_compulsory_assignment.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pl_compulsory_assignment"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="resultList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,400,0"/>
        <TextBox x:Name="startValue" HorizontalAlignment="Left" Margin="448,121,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="endValue" HorizontalAlignment="Left" Margin="607,121,0,0" TextWrapping="Wrap" Text="20000000" VerticalAlignment="Top" Width="120"/>
        <Label Content="Start Value" HorizontalAlignment="Left" Margin="448,90,0,0" VerticalAlignment="Top"/>
        <Label Content="End Value" HorizontalAlignment="Left" Margin="607,90,0,0" VerticalAlignment="Top"/>
        <Button Content="Start / Stop Sequential" HorizontalAlignment="Left" Margin="604,170,0,0" VerticalAlignment="Top" Click="buttonClick" IsDefault="True"/>
        <Button Content="Start / Stop Parallel" HorizontalAlignment="Left" Margin="604,0,0,0" VerticalAlignment="Center" Click="buttonClickTwo"/>
    </Grid>
</Window>
